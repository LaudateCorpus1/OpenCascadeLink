Cell[CellGroupData[{

Cell["Development setup", "Subsubsection",ExpressionUUID->"15f26c59-6d19-4abb-8b3a-929d6494e0fc"],

Cell["\<\
To quickly develop an test new OpenCascadeLink functionality two steps are \
most important. First, copy a modified OpenCascadeLink.m file into the paclet \
and second recompile the library link binary. You may omit this last step if \
if no changes to the C code were made.
To do so the following set up seems useful:\
\>", "Text",ExpressionUUID->"df904a15-942d-4fd6-b8a9-646909e176f6"],

Cell[BoxData[{
 RowBox[{
  RowBox[{"baseDir", "=", 
   RowBox[{"NotebookDirectory", "[", "]"}]}], ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"buildDir", "=", 
   RowBox[{"FileNameJoin", "[", 
    RowBox[{"{", 
     RowBox[{"baseDir", ",", "\"\<build\>\""}], "}"}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"pacletDir", "=", 
   RowBox[{"FileNameJoin", "[", 
    RowBox[{"{", 
     RowBox[{"buildDir", ",", "\"\<OpenCascadeLink\>\""}], "}"}], "]"}]}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"PacletDirectoryAdd", "[", "pacletDir", "]"}], 
  ";"}], "\[IndentingNewLine]", 
 RowBox[{
  RowBox[{"CopyFile", "[", 
   RowBox[{
    RowBox[{"FileNameJoin", "[", 
     RowBox[{"{", 
      RowBox[{
      "baseDir", ",", "\"\<OpenCascadeLink\>\"", ",", 
       "\"\<OpenCascadeLink.m\>\""}], "}"}], "]"}], ",", 
    RowBox[{"FileNameJoin", "[", 
     RowBox[{"{", 
      RowBox[{"pacletDir", ",", "\"\<OpenCascadeLink.m\>\""}], "}"}], "]"}], 
    ",", 
    RowBox[{"OverwriteTarget", "\[Rule]", "True"}]}], "]"}], ";"}], "\n", 
 RowBox[{"Get", "[", 
  RowBox[{"FileNameJoin", "[", 
   RowBox[{"{", 
    RowBox[{
    "baseDir", ",", "\"\<scripts\>\"", ",", 
     "\"\<BuildOpenCascadeLink.wl\>\""}], "}"}], "]"}], "]"}], "\n", 
 RowBox[{
  RowBox[{"setup", "=", 
   RowBox[{"Association", "[", 
    RowBox[{"{", 
     RowBox[{
      RowBox[{"\"\<BaseDirectory\>\"", "\[Rule]", "baseDir"}], ",", 
      RowBox[{"\"\<BuildDirectory\>\"", "\[Rule]", "buildDir"}]}], "}"}], 
    "]"}]}], ";"}], "\n", 
 RowBox[{
  RowBox[{"BuildOpenCascadeLink", "[", "setup", "]"}], ";"}]}], "Input",
 CellLabel->"In[1]:=",ExpressionUUID->"5495cc52-950c-4795-90dc-4d3e2b722627"],

Cell["\<\
If the C code was changes and a new library build then the kernel needs to be \
quit and restarted in order to load the changes otherwise a reloading the \
\[OpenCurlyDoubleQuote]OpenCascadeLink.m\[CloseCurlyDoubleQuote] file is \
sufficient.\
\>", "Text",ExpressionUUID->"e02e80c6-332e-4b84-b5ad-309fb2b94771"],

Cell[BoxData["Quit"], "Input",
 CellLabel->"In[11]:=",ExpressionUUID->"29027f99-4065-427d-a57c-28fb15456f1b"],

Cell[BoxData[
 RowBox[{"Get", "[", 
  RowBox[{"FileNameJoin", "[", 
   RowBox[{"{", 
    RowBox[{
     RowBox[{"NotebookDirectory", "[", "]"}], ",", "\"\<OpenCascadeLink\>\"", 
     ",", "\"\<OpenCascadeLink.m\>\""}], "}"}], "]"}], "]"}]], "Input",
 CellLabel->"In[11]:=",ExpressionUUID->"3255c058-aca5-465c-b567-9b51c0b53e7c"],

